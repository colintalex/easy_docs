<div class="container">
  <%= form_with url: "/login", method: :post do |form| %>
    <div class="form-group">
      <%= form.label :email, "Username:" %>
      <%= form.text_field :email, class: 'form-control', value: 'colintalex@gmail.com' %>
    </div>
    <div class="form-group">
      <%= form.label :password, "Password:" %>
      <%= form.text_field :password, class: 'form-control' %>
    </div>
    <%= form.submit "Search", class: 'btn btn-primary' %>
  <% end %>
</div>

<%= link_to 'SignUp', '/register' %>